#: data/nation/chilean.ruleset:4
msgid "?plural:Chileans"
msgstr "Чилийцы"

#: data/nation/chilean.ruleset:6
msgid "Chile is located on the south-western coast of South America."
msgstr "Чили находится на юго-западном берегу Южной Америки."

#: data/nation/chilean.ruleset:18
msgid "Captain General"
msgstr "Капитан-Генерал"

#: data/nation/chilean.ruleset:19 data/nation/french.ruleset:14
